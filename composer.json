
{
    "name": "conf09/saml2-bearer",
    "description": "This Pacakage Of CAS Apereo saml2 authentication with oauth2.0 for the authorization",
    "type": "library",
    "require": {
        "php": ">=5.5.0",
        "ext-openssl": "*",
        "illuminate/support": ">=5.0.0",
        "onelogin/php-saml": "^3.0.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Saml\\Bearer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saml\\Bearer\\Saml2BearerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
